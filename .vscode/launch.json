  "version": "0.2.0",
  "configurations": [
    {
      "name": "boilerplate (development)",
      "cwd": "boilerplate",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define", "ENVIRONMENT_TYPE=development"]
    },
    {
      "name": "boilerplate (staging)",
      "cwd": "boilerplate",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define", "ENVIRONMENT_TYPE=staging"]
    },
    {
      "name": "boilerplate (production)",
      "cwd": "boilerplate",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": ["--dart-define", "ENVIRONMENT_TYPE=production"]
    }
  ]
}
